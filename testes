        "class Point:\n",
        "  def __init__(self, x, y):\n",
        "    print(\"INIT\")\n",
        "    self.x = x\n",
        "    self.y = y\n",
        "  def __add__(self, other):\n",
        "    print(\"ADD\")\n",
        "    return Point(self.x + other.x, self.y + other.y)\n",
        "  def __eq__(self, other):\n",
        "    print(\"EQ\")\n",
        "    return self.x == other.x and self.y == other.y\n",
        "  def __str__(self):\n",
        "    print(\"STR\")\n",
        "    return f\"({self.x}, {self.y})\"\n",
        "\n",
        "p1 = Point(1, 2)\n",
        "p2 = Point(3, 4)\n",
        "\n",
        "p3 = p1 + p2\n",
        "print(p3)"
