      "cell_type": "code",
      "source": [
        "class Employee:\n",
        "  def __init__(self, name, salary):\n",
        "    self.name = name\n",
        "    self.salary = salary\n",
        "  def __str__(self):\n",
        "    return self.name + ' recebe ' + str(self.salary)\n",
        "  def proporcional(self, days):\n",
        "    return self.salary / 30 * days"
